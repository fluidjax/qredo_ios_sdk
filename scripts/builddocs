DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
cd $DIR
cd ../docs/
rm -rf objc
mkdir objc
cd ..
jazzy \
 --module QredoXDK \
 --objc \
 --clean \
 --sdk iphonesimulator \
 --author Qredo \
 --author_url http://qredo.com \
 --framework-root . \
 --source-directory . \
 --umbrella-header QredoSDK/include/Qredo.h \
 --output docs/objc \
 --theme apple \
 --hide-documentation-coverage 
 
